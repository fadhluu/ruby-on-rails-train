<%= content_for :title do %>
  New Contacts
<% end %>
<%= content_for :body do %>
  <%= form_for @contact do |f| %>
    <%= f.label :sosmed %>
    <%= f.text_field :sosmed, placeholder:'Sosmed'%>
    <%= @contact.errors[:sosmed].first unless @contact.errors[:sosmed].blank? %>
    <%= f.label :sosmed_username, "Sosmed Username" %>
    <%= f.text_field :sosmed_username, placeholder:'Sosmed'%>
    <%= f.submit :submit %>
  <% end %>
<% end %>
